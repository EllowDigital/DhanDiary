{
  "expo": {
    "owner": "ellowdigital",
    "name": "DhanDiary",
    "slug": "dhandiary",
    "scheme": "dhandiary",
    "version": "1.2.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "jsEngine": "hermes",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,

    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "ios": {
      "supportsTablet": true,
      "buildNumber": "122",
      "bundleIdentifier": "com.ellowdigital.dhandiary"
    },

    "android": {
      "package": "com.ellowdigital.dhandiary",
      "versionCode": 122,

      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "dhandiary"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],

      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },

    "web": {
      "favicon": "./assets/favicon.png"
    },

    "plugins": [
      "expo-updates",
      "expo-sqlite",
      "expo-font",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow DhanDiary to use FaceID to secure your data."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "enableHermes": true,
            "minSdkVersion": 24,
            "ndk": {
              "debugSymbolLevel": "NONE"
            },
            "abiFilters": ["arm64-v8a"]
          }
        }
      ]
    ],

    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/77c33e0a-8d3a-4f5b-afcf-72967c9af0bf"
    },

    "runtimeVersion": {
      "policy": "appVersion"
    },

    "extra": {
      "eas": {
        "projectId": "77c33e0a-8d3a-4f5b-afcf-72967c9af0bf"
      }
    }
  }
}
